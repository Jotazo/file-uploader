{
  "version": 2,
  "builds": [
    {
      "buildCommand": "npm run build",
      "installCommand": "npm i",
      "src": "build/app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["build/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/app.js"
    }
  ]
}
